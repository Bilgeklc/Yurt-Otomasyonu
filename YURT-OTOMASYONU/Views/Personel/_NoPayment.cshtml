@model IEnumerable<YURT_OTOMASYONU.Data.Odemeler>

<div class="container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Ad Soyad</th>
                <th>TC</th>
                <th>OdaNo</th>
                <th>Telefon Numarası</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                <td>@item.Ogrenci.Ad @item.Ogrenci.Soyad</td>
                <td>@item.Ogrenci.TCKimlikNo</td>
                <td>@item.Ogrenci.OdaNo</td>
                <td>@item.Ogrenci.TelefonNo</td>
                </tr>
            }
        </tbody>
    </table>
</div>
<nav style="align-items:center">
    <ul style="place-content: center" class="pagination">
        <li class="page-item disabled">
            <a class="page-link" href="#" tabindex="-1">Geri</a>
        </li>
        <li class="page-item active"><a class="page-link" href="#">1</a></li>
        <li class="page-item ">
            <a class="page-link" href="#">2 <span class="sr-only">(current)</span></a>
        </li>
        <li class="page-item"><a class="page-link" href="#">3</a></li>
        <li class="page-item">
            <a class="page-link" href="#">İleri</a>
        </li>
    </ul>
</nav>